{
    "algorithm": "OPENSSL",
    "blacklist": [
    ],
    "chmod": "0755",
    "compression": "GZ",
    "directories": [
        "config",
        "src"
    ],
    "files": [
        "LICENSE.md"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["tests", "test"],
            "in": "vendor"
        }
    ],
    "git-version": "package_version",
    "intercept": true,
    "key": "<path to openssl private key>",
    "main": "bin/component-installer.php",
    "output": "component-installer.phar",
    "stub": true
}
